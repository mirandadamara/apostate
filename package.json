{
  "name": "apostate",
  "version": "0.0.8",
  "description": "Reactive state management.",
  "main": "lib/index.js",
  "scripts": {
    "test": "tape 'lib/**/*-spec.js' | faucet",
    "start": "npm run build-example && http-server example",
    "build": "babel src --out-dir lib",
    "build-watch": "babel src --watch --out-dir lib",
    "build-example": "browserify example/client -t babelify > example/build/client.js",
    "build-benchmark": "browserify benchmark/index -t babelify > benchmark/bundle.js",
    "benchmark": "npm run build-benchmark && http-server benchmark"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zebulonj/apostate.git"
  },
  "author": "Zebulon Young (zebulonj@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zebulonj/apostate/issues"
  },
  "homepage": "https://github.com/zebulonj/apostate",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babelify": "^6.1.2",
    "blue-tape": "^0.1.10",
    "browserify": "^10.2.4",
    "faucet": "0.0.1",
    "http-server": "^0.8.0",
    "tape": "^4.0.0",
    "webpack": "^1.11.0"
  },
  "dependencies": {
    "immutable": "^3.7.4",
    "lodash": "^3.10.0",
    "page": "^1.6.3",
    "rx": "^2.5.3"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
